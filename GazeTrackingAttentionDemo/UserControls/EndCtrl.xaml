<UserControl x:Class="GazeTrackingAttentionDemo.UserControls.EndCtrl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:GazeTrackingAttentionDemo.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"
             Loaded="onLoad">
    <Grid HorizontalAlignment="Center" VerticalAlignment="Center">
        <StackPanel>
            <Button x:Name="Back" Height="20"  Margin="0,0, 0,10" VerticalAlignment="Top" Click="Back_Click" >Back</Button>
        <Button x:Name="end" Height="20" Margin="0,10, 0,0" VerticalAlignment="Top" Click="End_Click">Exit Program</Button>
        </StackPanel>
    </Grid>
</UserControl>
