<Window x:Class="GazeTrackingAttentionDemo.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GazeTrackingAttentionDemo"
        mc:Ignorable="d"
        Title="Window1"
        WindowState="Maximized"
        Width="1920"
        Height="1080"
        WindowStyle="SingleBorderWindow">
    <Window.Resources>
        <ObjectDataProvider ObjectType="{x:Type local:BuildA4}"
            MethodName="getA4Height" x:Key="a4height"/>
        <ObjectDataProvider ObjectType="{x:Type local:BuildA4}"
            MethodName="getA4Width" x:Key="a4width"/>
    </Window.Resources>
    <Grid>
        <WebBrowser x:Name="DocumentArea"
            Width="{Binding Source={StaticResource a4width, PresentationTraceSources.TraceLevel=High}}" 
            Height="{Binding Source={StaticResource a4height}}" 
            SnapsToDevicePixels="True"
                 Margin="560.54,0,560.54,-69.74"
                 />

    </Grid>
</Window>
